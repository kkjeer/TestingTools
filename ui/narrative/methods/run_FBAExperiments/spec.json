{
  "ver": "0.0.1",
  "authors": ["kkjeer"],
  "contact": "",
  "categories": ["active"],
  "widgets": {
    "input": null,
    "output": "no-display"
  },
  "parameters": [
    {
      "text_options": {
        "valid_ws_types": ["KBaseFBA.FBAModel"]
      },
      "advanced": false,
      "default_values": [""],
      "id": "fbamodel_id",
      "optional": false,
      "allow_multiple": false,
      "field_type": "text"
    },
    {
      "text_options": {
        "valid_ws_types": ["KBaseBiochem.Media", "KBaseBiochem.MediaSet"]
      },
      "advanced": false,
      "default_values": [""],
      "id": "media_id",
      "optional": true,
      "allow_multiple": false,
      "field_type": "text"
    },
    {
      "id": "compound_id",
      "default_values": [""],
      "advanced": false,
      "optional": false,
      "allow_multiple": false,
      "field_type": "dynamic_dropdown",
      "dynamic_dropdown_options": {
        "data_source": "custom",
        "service_function": "BiochemistryAPI.search_compounds",
        "service_version": "release",
        "service_params": [{ "query": "{{dynamic_dropdown_input}}" }],
        "selection_id": "id",
        "description_template": "<strong>{{id}}</strong>: {{name}}"
      }
    }
  ],
  "parameter-groups": [
    {
      "id": "experiments",
      "parameters": ["compound_id"],
      "optional": false,
      "allow_multiple": true,
      "with_border": true
    }
  ],
  "behavior": {
    "service-mapping": {
      "url": "",
      "name": "TestingTools",
      "method": "run_FBAExperiments",
      "input_mapping": [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "narrative_system_variable": "workspace_id",
          "target_property": "workspace_id"
        },
        {
          "input_parameter": "fbamodel_id",
          "target_property": "fbamodel_id"
        },
        {
          "input_parameter": "media_id",
          "target_property": "media_id"
        },
        {
          "input_parameter": "experiments",
          "target_property": "experiments"
        }
      ],
      "output_mapping": [
        {
          "service_method_output_path": [0, "report_name"],
          "target_property": "report_name"
        },
        {
          "service_method_output_path": [0, "report_ref"],
          "target_property": "report_ref"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}
